<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #1D2228;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="!!!Congratulations, Winners!!!" textAlignment="CENTER" textFill="#e1e2e2">
         <font>
            <Font name="Berlin Sans FB Bold" size="24.0" />
         </font></Label>
      <VBox fx:id="vBoxWinners" alignment="TOP_CENTER" spacing="5.0" />
      <Button fx:id="btnOk" mnemonicParsing="false" prefWidth="70.0" style="-fx-background-color: FB8122; -fx-background-radius: 10 10 10 10;" text="Ok" textFill="#e1e2e2" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
