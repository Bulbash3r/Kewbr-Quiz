<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" prefHeight="100.0" prefWidth="250.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Write path to pack" />
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <TextField fx:id="txtFieldPath" />
            <Button fx:id="btnChoose" mnemonicParsing="false" text="Choose" />
         </children>
      </HBox>
      <Button fx:id="btnOk" mnemonicParsing="false" prefWidth="56.0" text="Ok" />
   </children>
</VBox>
